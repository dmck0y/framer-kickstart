{
  "name": "framer-projects",
  "version": "1.0.0",
  "description": "Framer starter project",
  "scripts": {
    "build": "browserify -t coffeeify --extension='.coffee' src/js/app.coffee > compiled/app.js",
    "watch": "watchify ./src/js/app.coffee -t coffeeify -o ./compiled/app.js",
    "browser-sync": "browser-sync start --files 'compiled/app.js' --server",
    "start-dev": "npm run watch & npm run browser-sync"
  },
  "devDependencies": {},
  "dependencies": {
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "coffeeify": "^2.0.1",
    "framerjs": "git+ssh://git@github.com/koenbok/Framer.git",
    "lodash-fp": "^0.10.4",
    "watchify": "^3.7.0"
  }
}
